{
   "action": {
      "default_icon": {
         "128": "images/icon.jpg"
      }
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "sidepanel.js" ],
      "matches": [ "\u003Call_urls>" ]
   } ],
   "content_security_policy": {
      "extension_pages": "script-src 'self'; object-src 'self';"
   },
   "description": "A Chrome extension to bulk download images from a list of image URLs. Schedule downloads, filter by image type, save in folder, etc.",
   "host_permissions": [ "https://bulkimagedownloaderurllist.com/*" ],
   "icons": {
      "128": "images/icon.jpg"
   },
   "key": "",
   "manifest_version": 3,
   "name": "Bulk Image Downloader From Url List",
   "options_page": "options.html",
   "permissions": [ "downloads", "storage", "alarms", "cookies", "sidePanel", "activeTab", "scripting", "tabs", "webNavigation" ],
   "side_panel": {
      "default_path": "sidepanel.html"
   },
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "3.0",
   "web_accessible_resources": [ {
      "matches": [ "\u003Call_urls>" ],
      "resources": [ "redirection/redirect-checker.html", "redirection/redirect-checker.js", "sidepanel.html", "sidepanel.js", "dist/jszip.min.js", "dist/image-conversion.js", "dist/jquery.watermark.js" ]
   } ]
}
